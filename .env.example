# Copie para .env e preencha os valores
# cp .env.example .env

# Banco de dados
N8N_DB_NAME=n8n_db
N8N_DB_USER=n8n
N8N_DB_PASSWORD=SUA_SENHA_POSTGRES

# Autenticação do n8n
N8N_BASIC_AUTH_ACTIVE=true
N8N_BASIC_AUTH_USER=admin
N8N_BASIC_AUTH_PASSWORD=SUA_SENHA_N8N

# Host e protocolo (IP ou domínio da VPS)
N8N_HOST=n8n.construsoft.online
N8N_PORT=5678
N8N_PROTOCOL=https

# Obrigatório para webhooks (Telegram, etc.): URL pública em HTTPS, SEM porta.
# Telegram só aceita portas 80, 88, 443 ou 8443.
N8N_WEBHOOK_URL=https://n8n.construsoft.online

N8N_EXECUTIONS_DATA_SAVE_ON_SUCCESS=all
N8N_EXECUTIONS_DATA_SAVE_ON_ERROR=all
N8N_EXECUTIONS_TIMEOUT=3600
N8N_GENERIC_FUNCTION_TIMEOUT=120
